{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "related_blogs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog.blog"
    },
    "ads_cards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::welcome-bonus-card.welcome-bonus-card"
    }
  }
}
